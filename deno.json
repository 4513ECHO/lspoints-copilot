{
  "compilerOptions": {
    "exactOptionalPropertyTypes": false,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true,
    "useUnknownInCatchVariables": true
  },
  "lint": {
    "rules": {
      "include": [
        "eqeqeq",
        "explicit-function-return-type"
      ]
    }
  },
  "lock": false,
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check ."
  }
}
